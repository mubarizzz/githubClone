                <% if(results){ %>
                    <% for(var i=0; i< total; i++) {%>
                        <form action="/deleteItem" method="POST">
                        <div class="repository">
                            <div class="gg">
                                <% if(results[i].itemType == "file"){ %>
                                <a href="#"><i class="fas fa-file"></i> <%= results[i].itemName %></a>
                                <% } else {%>
                                <a href="#"><i class="fas fa-folder"></i> <%= results[i].itemName %></a>
                                <% } %>
                            </div>
                            <div class="ff"><a href="#"> <%= results[i].commitMessage %></a></div>
                            <div>5 years ago</div>
                          </div>
                        </form>
